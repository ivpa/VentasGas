<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/fondoproducto4"
    tools:context=".EmisionEliminarActivity" >

    <TextView
        android:id="@+id/tituloemisioneliminar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="150sp"
        android:layout_marginTop="150sp"
        android:text="@string/EmisionEliminar"
        android:textSize="@dimen/size1" />
    
     <TextView
        android:id="@+id/idemisioneliminar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/idemision"
        android:textSize="@dimen/size1" />

    <EditText
        android:id="@+id/IdEmisionEliminar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/IDEmisionVale"
        android:maxLength="11" >

        <requestFocus />
    </EditText>
    <Button
        android:id="@+id/botonemisioneliminar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="20dp"
        android:text="@string/eliminar"
        android:textSize="@dimen/size1"
        android:onClick="eliminarEmision" />

</LinearLayout>
